<form action= "<%= notes_url %>" method="post">
  <%= auth_token %>
  <input type="hidden" name="note[track_id]" value="<%= @track.id%>">
  <label for="note">Enter note below:</label>
  <br>
  
  <textarea rows="8" cols="40" name="note[note]" id="note"></textarea>
  <br>
  <button>Submit</button>
</form>
<br>
